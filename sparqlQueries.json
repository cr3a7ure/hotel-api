PREFIX schema: <http://schema.org/>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
prefix hydra: <http://www.w3.org/ns/hydra/core#>
#prefix : <>
# SELECT EXACT CLASSSS
#   describe ?class
select distinct ?class ?entrypoint
FROM <http://localhost:8090/test1/data/apiv7>
WHERE  {
  ?class rdf:type schema:test. # Retrieve the correct class
  ?server hydra:supportedClass ?class. # find the server
  ?server hydra:entrypoint ?entrypoint . # retrieve the servers entrypoint
  ?class hydra:supportedProperty ?props .  # class qualifies a list of props
  ?props hydra:property ?property . # list of props we need

} VALUES ?property {
 schema:longitude
 schema:latitude
}

//ANSWER
{
  "head": {
    "vars": [ "class" , "entrypoint" ]
  } ,
  "results": {
    "bindings": [
      {
        "class": { "type": "uri" , "value": "http://localhost:8091/geo_coordinates" } ,
        "entrypoint": { "type": "literal" , "value": "/" }
      }
    ]
  }
}

